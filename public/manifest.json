{
  "name": "ZoTsu",
  "version": "0.1",
  "description": "ZuTsu's chrome extension",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/logo.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": ["activeTab", "storage", "webNavigation", "https://rent.591.com.tw/*"],
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://rent.591.com.tw/*"],
      "js": ["js/content_script.js"]
    }
  ]
}
